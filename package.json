{
  "name": "nuxt-graphql-server",
  "version": "1.1.4",
  "license": "MIT",
  "repository": "https://github.com/productdevbook/nuxt-graphql-server-module",
  "homepage": "https://github.com/productdevbook/nuxt-graphql-server-module",
  "type": "module",
  "author": {
    "name": "Mehmet - productdevbook",
    "email": "hi@productdevbook.com"
  },
  "contributors": [
    "Intevel <hi@productdevbook.com>"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "nuxt-module-build && standard-version && git push --follow-tags && npm publish",
    "lint": "eslint --ext .js,.ts,.vue"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "standard-version": "^9.3.2"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "eslint": "latest",
    "nuxt3": "latest"
  },
  "keywords": [
    "nuxt",
    "graphql",
    "nuxtjs",
    "nuxt-module",
    "vue3",
    "nuxt3",
    "graphql-module",
    "nuxt-graphql",
    "nuxtgraphql",
    "nuxt-community"
  ],
  "packageManager": "yarn@3.2.1"
}
